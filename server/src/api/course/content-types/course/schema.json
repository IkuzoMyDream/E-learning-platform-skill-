{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "enrollers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "courses"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "courses"
    },
    "picture": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "name_teacher": {
      "type": "string"
    },
    "mail_teacher": {
      "type": "email"
    },
    "price": {
      "type": "integer"
    },
    "carts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart.cart",
      "mappedBy": "course"
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment",
      "mappedBy": "course"
    },
    "progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::progress.progress",
      "mappedBy": "course"
    },
    "phone_number": {
      "type": "string"
    },
    "course_chapters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-chapter.course-chapter",
      "mappedBy": "course"
    },
    "course_materials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::material.material",
      "mappedBy": "course"
    }
  }
}
