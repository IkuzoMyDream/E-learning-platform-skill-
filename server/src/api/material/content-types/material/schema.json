        "files",
        "videos",
        "audios"
      ]
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "duration": {
      "type": "integer"
    },
    "course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course.course",
      "inversedBy": "materials"
    },
    "chapter_number": {
      "type": "integer"
    },
    "progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::progress.progress",
      "mappedBy": "material"
